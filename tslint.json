{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": [ true ],
      "semicolon": [true, "never"],
      "quotemark": [true, "single"],
      "whitespace": true,
      "space-before-function-paren": true,
      "space-within-parens": true,
      "import-spacing": [true],
      "no-trailing-whitespace": true,
      "no-irregular-whitespace": true,
      "no-unused-variable": true,
      "variable-name": [false]
  },
  "rulesDirectory": []
}